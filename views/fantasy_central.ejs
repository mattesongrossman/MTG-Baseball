    <div class="container-fluid">
      <!-- Example DataTables Card-->
      <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i> 2018 Steamer: Expert Player Projections</div>
          <div class="card-header">
             <div>
               <ol class="breadcrumb">
                       <li class="breadcrumb-item">
                         <a href="#" onclick = "logMe();">Batters</a>
                       </li>
                       <li class="breadcrumb-item active">
                         <a href="#">Pitchers</a>
                       </li>
              </ol>
             </div>
           </div>
          <div class="col-sm-12 col-md-6">
            <div id="dataTable_filter" class="dataTables_filter">
              <label>Player Search:
              <span class="input-group-append">
              <input type="search" class="form-control form-control-sm" placeholder="" aria-controls="dataTable"></label>
              <button id="search_button"class="btn btn-primary" type="button">
                <i class="fa fa-search"></i>
              </button>
            </span>
            </div>
            </div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Team</th>
                  <th>AVG</th>
                  <th>OBP</th>
                  <th>SLG</th>
                  <th>OPS</th>
                  <th>SB</th>
                  <th>wOBA</th>
                  <th>WAR</th>
                  <th>ADP</th>
                  <th>Drafted?</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>Name</th>
                  <th>Team</th>
                  <th>AVG</th>
                  <th>OBP</th>
                  <th>SLG</th>
                  <th>OPS</th>
                  <th>SB</th>
                  <th>wOBA</th>
                  <th>WAR</th>
                  <th>ADP</th>
                  <th>Drafted?</th>
                </tr>
              </tfoot>
              <tbody>
                <% for(let i=0; i< playerData.length ; i++) { %>
                <tr>
                <td><a href = "https://www.fangraphs.com/statss.aspx?playerid=<%=playerData[i].playerid%>">
                  <%=playerData[i].name%></a></td>
                <td><%=playerData[i].team%></td>
                <td><%=playerData[i].avg%></td>
                <td><%=playerData[i].obp%></td>
                <td><%=playerData[i].slg%></td>
                <td><%=playerData[i].ops%></td>
                <td><%=playerData[i].sb%></td>
                <td><%=playerData[i].woba%></td>
                <td><%=playerData[i].war%></td>
                <td><%=playerData[i].adp%></td>
                <td onclick = "this.style.background = '#007bff'"></td>
                </tr>
                <% } %>
              </tbody>
            </table>
          </div>
        </div>
        <div class="card-footer small text-muted">Brought to you by...</div>
      </div>
